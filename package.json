  "scripts": {
    "webpack-product": "webpack --progress --watch --colors",
    "webpack-dev": "webpack",
    "run-server": "nodemon -x babel-node ./server/dist/server/index.js",
    "initDB": "mysql -u root -p myHome < ./scripts/initDB.sql",
    "webpack-server": "webpack && yarn run-server",
    "test": "jest",
    "createDoc": "typedoc --out ./doc/ ./src/index.tsx --module commonjs --jsx jsx --lib lib.es6.d.ts",
    "ts-dev": "tsc gulpfile.ts webpack.config.ts webpack.dev.ts --allowJs && gulp dev",
    "ts-build": "tsc gulpfile.ts webpack.config.ts webpack.prod.ts --allowJs && gulp build",
    "forever-start":"forever start -l forever.log -o server.log -e error.log -a ./server/dist/server/index.js",
    "forever-stop":"forever stop ./server/dist/server/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(css|less|scss)$": "<rootDir>/CSSStub.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "/scss/"
    ]
  }
